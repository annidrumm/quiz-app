text-transform: uppercase;\n  }\n}\n","src/scss/components/_footer.scss":".footer {\n  background-color: #86a5d9;\n  padding: 15px;\n  width: auto;\n  margin: 20px 0 0 0;\n  position: fixed;\n  bottom: 0;\n  left: 0;\n  right: 0;\n\n  &__nav {\n    display: flex;\n    text-align: center;\n    justify-content: space-around;\n    align-items: center;\n    width: 100%;\n    max-width: 100%;\n\n    img {\n      width: 100%;\n      height: 40px;\n    }\n  }\n}\n\n.svg-filled {\n  fill: #86a5d9;\n  color: black;\n  width: 40px;\n  height: 40px;\n}\n\n.svg-filled-active {\n  fill: black;\n  color: black;\n  width: 40px;\n  height: 40px;\n}\n","src/scss/components/_card.scss":".card {\n  background-color: white;\n  border-radius: 10px;\n  padding: 30px;\n  border-style: solid;\n  border-color: #a8c2f07b;\n  margin: 0 20px 60px 20px;\n  position: relative;\n  box-shadow: 6px 4px 6px rgba(1, 57, 95, 0.2);\n\n  &answer-button--hide {\n    content: 'Hide answer';\n  }\n\n  &__answer-container {\n    text-align: center;\n  }\n\n  &__headline {\n    font-weight: bolder;\n  }\n\n  &__bookmark-button {\n    position: absolute;\n    top: 0;\n    right: 1.5rem;\n    height: 70px;\n    width: 50px;\n    -webkit-transform: rotate(0deg) skew(0deg);\n    transform: rotate(0deg) skew(0deg);\n    border-left: 25px solid #a8c2f065;\n    border-right: 25px solid #a8c2f065;\n    border-bottom: 25px solid white;\n\n    &--active {\n      border-left: 25px solid #a387cf;\n      border-right: 25px solid #a387cf;\n    }\n  }\n}\n","src/scss/components/_tag.scss":".tag-list {\n  list-style-type: none;\n  line-height: 2em;\n  position: relative;\n  left: auto;\n  right: auto;\n}\n\n.tag-list li {\n  display: inline-block;\n  padding: 5px 20px;\n  border: 1px solid #a8c2f065;\n  border-radius: 8px;\n  background-color: #a8c2f065;\n  margin: 10px;\n  white-space: nowrap;\n}\n","src/scss/components/_profile.scss":".profile__intro {\n  display: flex;\n\n  &--image {\n    border-radius: 50%;\n    width: 150px;\n    height: 150px;\n    border: 4px solid lightgrey;\n    display: block;\n  }\n\n  &--name {\n    margin-left: 20px;\n  }\n}\n\n.logout {\n  text-align: center;\n}\n","src/scss/components/_create.scss":".create {\n  text-align: center;\n  margin-bottom: 80px;\n\n  &__form {\n    display: flex;\n    flex-direction: column;\n\n    &--input {\n      margin: 10px 0 30px 0;\n    }\n  }\n\n  &__label {\n    font-weight: bolder;\n  }\n}\n\n#question,\n#answer,\n#tags {\n  font-family: 'Roboto', sans-serif;\n}\n","src/scss/components/_button.scss":".submit-button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.logout__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.card__answer-button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.light__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.night__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.light-mode {\n  background: radial-gradient(\n    circle,\n    rgba(134, 165, 217, 1) 47%,\n    rgba(95, 75, 182, 1) 71%\n  );\n  content: 'Hide answer';\n}\n\n.displaynone {\n  display: none;\n}\n"},"lineCount":null}},"error":null,"hash":"9c4c08538079758f0fb1f6dc4a87b791","cacheData":{"env":{}}}