n;\n\n    &--input {\n      margin: 10px 0 30px 0;\n    }\n  }\n\n  &__label {\n    font-weight: bolder;\n  }\n}\n\n#question,\n#answer,\n#tags {\n  font-family: 'Roboto', sans-serif;\n}\n","src/scss/components/_button.scss":".submit-button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.logout__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.card__answer-button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.light__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.night__button {\n  background-color: #a387cf;\n  display: inline-block;\n  padding: 10px 25px 10px 25px;\n  margin: auto;\n  border-radius: 7px;\n  font-size: 80%;\n  cursor: pointer;\n}\n\n.light-mode {\n  background: radial-gradient(\n    circle,\n    rgba(134, 165, 217, 1) 47%,\n    rgba(95, 75, 182, 1) 71%\n  );\n  content: 'Hide answer';\n}\n\n.displaynone {\n  display: none;\n}\n"},"lineCount":null}},"error":null,"hash":"ba589a607c364383eb045a2a9fbc4ac5","cacheData":{"env":{}}}